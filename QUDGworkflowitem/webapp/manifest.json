                                            "applicablePath": "ApproveButton"
                                        },
                                        "ShowLog": {
                                            "id": "ShowLog",
                                            "text": "Show Log",
                                            "press": "onShowLog",
                                            "requiresSelection": false,
                                            "determining": true
                                        },
                                        "ShowStatus": {
                                            "id": "ShowStatus",
                                            "text": "Show Status",
                                            "press": "onShowStatus",
                                            "requiresSelection": false,
                                            "determining": true
                                        },
                                        "Submit": {
                                            "id": "Submit",
                                            "text": "Submit",
                                            "press": "onApprove",
                                            "requiresSelection": false,
                                            "determining": true,
                                            "applicablePath": "EditButton"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
