<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core" templateShareable="false" class="sapUiTinyMarginTop">
    <Dialog title="Document Details" class="sapUiSmallMargin">
        <content>
            <List items="{/items}">
                <DisplayListItem label="{label}" value="{value}" />
            </List>
        </content>
        <endButton>
            <Button text="Cancel" press="closeInvoiceFragment" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
