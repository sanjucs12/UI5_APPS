<mvc:View controllerName="processflow.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:pf="sap.suite.ui.commons">
    <Page id="page" title="{i18n>title}">
        <content >
            <pf:ProcessFlow id="processflow1" scrollable="false" foldedCorners="true" nodePress="onNodePress" nodes="{JsonModel>/Nodes}" lanes="{JsonModel>/Lanes}">
                <pf:nodes>
                    <pf:ProcessFlowNode laneId="{JsonModel>lane}" nodeId="{JsonModel>id}" title="{JsonModel>title}" titleAbbreviation="{JsonModel>titleAbbreviation}" children="{JsonModel>children}" state="{JsonModel>state}" stateText="{JsonModel>stateText}" texts="{JsonModel>texts}" highlighted="{JsonModel>highlighted}" focused="{JsonModel>focused}" />
                </pf:nodes>
                <pf:lanes>
                    <pf:ProcessFlowLaneHeader laneId="{JsonModel>id}" iconSrc="{JsonModel>icon}" text="{JsonModel>label}" position="{JsonModel>position}" />
                </pf:lanes>
            </pf:ProcessFlow>
        </content>
    </Page>
</mvc:View>
